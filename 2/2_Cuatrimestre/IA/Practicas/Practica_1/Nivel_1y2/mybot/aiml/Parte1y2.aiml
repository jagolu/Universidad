<category>
    <pattern>ES <set>pelos</set></pattern>
    <template>
        <think>
            <set var="pelo"><star/></set>
            <set var="nuestro"><map name="pelo"><get name="el_nombre"/></map></set>
        </think>
        <condition var="pelo">
            <li value="moreno">
                <condition var="nuestro">
                    <li value="moreno">Si</li>
                    <li>No</li>
                </condition>
            </li>
            <li value="morena">
                <condition var="nuestro">
                    <li value="moreno">Si</li>
                    <li>No</li>
                </condition>
            </li>
            <li value="blanco">
                <condition var="nuestro">
                    <li value="blanco">Si</li>
                    <li>No</li>
                </condition>
            </li>
            <li value="rubio">
                <condition var="nuestro">
                    <li value="rubio">Si</li>
                    <li>No</li>
                </condition>
            </li>
            <li value="rubia">
                <condition var="nuestro">
                    <li value="rubio">Si</li>
                    <li>No</li>
                </condition>
            </li>
            <li value="pelirrojo">
                <condition var="nuestro">
                    <li value="pelirrojo">Si</li>
                    <li>No</li>
                </condition>
            </li>
            <li value="pelirroja">
                <condition var="nuestro">
                    <li value="pelirrojo">Si</li>
                    <li>No</li>
                </condition>
            </li>
            <li value="calvo">
                <condition var="nuestro">
                    <li value="calvo">Si</li>
                    <li>No</li>
                </condition>
            </li>
            <li value="calva">
                <condition var="nuestro">
                    <li value="calvo">Si</li>
                    <li>No</li>
                </condition>
            </li>
            <li value="marron">
                <condition var="nuestro">
                    <li value="marron">Si</li>
                    <li>No</li>
